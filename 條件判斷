#include <bits/stdc++.h>
using namespace std;

int main() {
    int score;
    cout<<"please enter an integer";
    while(cin>>score){
        if(score>=60){
            cout<<"pass!!\n";
        }
        else if(score>=50 && score<60){
            cout<<"再努力一下!!\n";
        }
        else{
            cout<<"fail!!\n";
        }
    }
}
